 Third question answers : 
                            
                            UPDATE employee
                           	SET birthday = '1951-05-21'
                           	WHERE id = 18
                           	RETURNING *
                           
                           SELECT * FROM employee
                           
                           UPDATE employee
                           	SET email = 'Isaak@Charles.com'
                           	WHERE last_name = 'Charles'
                           	RETURNING *
                           
                           UPDATE employee
                           	SET email = 'helavelavelvela@gmail.com'
                           	WHERE last_name ILIKE 'd%t'
                           	RETURNING *
                           
                           UPDATE employee
                           	SET first_name = 'Rodinde',
                           		last_name = 'Trepels',
                           		birthday = '1997-09-13',
                           		email = 'sarÄ±@pipi.com'
                           	WHERE id = 12
                           RETURNING *
                           
                           UPDATE employee
                           	SET first_name = 'Baran',
                           		last_name = 'Alkan'
                           	WHERE id = 50
                           	RETURNING *
                            
    
Fourth question answers : 

                          DELETE FROM employee
                           	WHERE first_name ILIKE '%k'
                           	RETURNING *
                          
                           DELETE FROM employee
                           	WHERE email ILIKE 'b%'
                           	RETURNING *
                          
                           DELETE FROM employee
                           	WHERE id = 1
                           	RETURNING *
                          
                           DELETE FROM employee
                          	WHERE last_name ILIKE 'C%'
                          	RETURNING *
                          
                           DELETE FROM employee
                           	WHERE first_name LIKE 'A%'
                           	RETURNING *

 
 
